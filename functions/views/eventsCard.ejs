<% for(var i=0;i<Object.keys(events).length;i++) { %>
	<div class="grid">
		<div class="col s12 m6">
			<div
				class="card blue-grey darken-1 element-item <%= events[i].eventState %> <%= events[i].eventSport %>"
			>
				<div class="card-content white-text">
					<span class="card-title"><%= events[i].eventName %></span>
				</div>
				<div class="card-action">
					<a data-target="modal_<%= i %>" class="btn modal-trigger"
						>Read More</a
					>
				</div>
			</div>
		</div>
	</div>
	<div id="modal_<%= i %>" class="modal">
		<div class="modal-content">
			<h4>Event Details</h4>
			<p>
				<b>Sport</b> : <%= events[i].eventSport %> <br />
				<b>State</b> : <%= events[i].eventState %> <br />
				<b>Location</b> : <%= events[i].eventLocation %> <br />
				<b>Date</b> : <%= events[i].eventDate %> <br />
				<b>Email (You can contact here for more details)</b> : <%=
				events[i].contactEmail %> <br />
				<b>Phone Number (You can contact here for more details)</b> :
				<%= events[i].contactNumber %> <br />
			</p>
		</div>
		<div class="modal-footer">
			<a href="" class="waves-effect waves-green btn-flat">Register</a>
			<a href="#!" class="modal-close waves-effect waves-green btn-flat"
				>Close</a
			>
		</div>
	</div>
	<% }%>
