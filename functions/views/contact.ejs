	<body onload="Materialize.toast('Loading...', 1000)">
		<header>
			<nav>
				<ul id="slide-out" class="side-nav">
					<li>
						<img class="responsive-img" src="/images/logo.png" />
					</li>
					<li><a id="accountName" class="waves-effect"></a></li>
					<li>
